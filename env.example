# Configuração da aplicação
NODE_ENV=production
PORT=8080

# Database
DATABASE_URL="postgresql://username:password@host:port/database?schema=public"

# JWT
JWT_SECRET="your-super-secret-jwt-key-here"
JWT_EXPIRES_IN="7d"

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_CALLBACK_URL=http://localhost:3001/api/v1/auth/google/callback

# Frontend URLs
FRONTEND_URL=http://localhost:3000

# RabbitMQ (Mensageria)
RABBITMQ_URL=amqp://localhost:5672
# Para produção com autenticação:
# RABBITMQ_URL=amqp://user:password@rabbitmq.host:5672

# Push Notifications (Firebase Cloud Messaging)
FCM_PROJECT_ID=your-firebase-project-id
FCM_PRIVATE_KEY_ID=your-private-key-id
FCM_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n"
FCM_CLIENT_EMAIL=firebase-adminsdk-xxx@project.iam.gserviceaccount.com
FCM_CLIENT_ID=your-client-id

# SMS (Twilio)
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# Email (SendGrid)
SENDGRID_API_KEY=SG.xxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
SENDGRID_FROM_EMAIL=noreply@yourapp.com

# Redis (opcional - para cache/sessions)
REDIS_URL="redis://username:password@host:port"

# CORS
CORS_ORIGIN="https://your-frontend-domain.com"
WEBSOCKET_CORS_ORIGIN="https://your-frontend-domain.com"

# Cloud Run específico
# Estas variáveis são automaticamente definidas pelo Cloud Run
# GOOGLE_CLOUD_PROJECT=your-project-id
# K_SERVICE=fila-api
# K_REVISION=fila-api-xxxxx
# K_CONFIGURATION=fila-api 